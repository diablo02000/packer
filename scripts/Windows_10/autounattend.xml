							<Format>NTFS</Format>
							<Extend>false</Extend>
							<Active>true</Active>
						</ModifyPartition>
					</ModifyPartitions>
				</Disk>
			</DiskConfiguration>
			<ImageInstall>
				<OSImage>
					<InstallTo>
